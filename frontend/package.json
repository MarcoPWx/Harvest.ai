{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:mock": "NEXT_PUBLIC_ENABLE_MSW=1 next dev -p 3002",
    "dev:e2e": "NEXT_PUBLIC_ENABLE_MSW=1 NEXT_PUBLIC_E2E=1 next dev -p 3002",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "storybook:profile": "storybook build --profile",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:json": "playwright test --reporter=json > public/playwright-summary.json",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test && npm run test:e2e",
    "test:ci": "npm run test -- --ci --coverage && npm run test:e2e",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:smoke:e2e": "playwright test tests/e2e/smoke.spec.ts tests/e2e/s2s-api.spec.ts --project=chromium",
    "test:e2e:smoke:tags": "playwright test --grep @smoke",
    "test:all:fast": "npm test && npm run test:smoke:e2e",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:sb": "playwright test -c playwright.storybook.config.ts",
    "test:sb:headed": "playwright test -c playwright.storybook.config.ts --headed",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-storybook:subpath": "SB_BASE=/storybook/ storybook build",
    "storybook:export": "npm run build-storybook:subpath && node scripts/copy-storybook-to-public.mjs",
    "docs:overview:pdf": "npm run build-storybook && node scripts/export-overview-pdf.mjs",
    "storybook:analyze": "ANALYZE=1 storybook build",
    "storybook:split": "SPLIT_CHUNKS=1 storybook build",
    "storybook:analyze:split": "SPLIT_CHUNKS=1 ANALYZE=1 storybook build",
    "storybook:test": "storybook test",
    "docs:manifest": "node scripts/generate-docs-manifest.mjs",
    "docs:updates": "node scripts/generate-docs-updates.mjs",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run test:all",
    "smoke": "node scripts/smoke.mjs",
    "smoke:try": "SMOKE_TRY_GENERATE=1 node scripts/smoke.mjs",
    "prepare": "husky",
    "commit": "cz",
    "lint-staged": "lint-staged",
    "dev:all": "concurrently -k -n APP,STORYBOOK -c blue,magenta \"npm run dev:mock\" \"npm run storybook\""
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@google/generative-ai": "^0.24.1",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@stripe/stripe-js": "^7.9.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.56.0",
    "@tanstack/react-query": "^5.85.5",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.542.0",
    "next": "15.5.0",
    "openai": "^5.16.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-markdown": "^9.0.3",
    "recharts": "^3.1.2",
    "resend": "^6.0.1",
    "sonner": "^2.0.7",
    "stripe": "^18.5.0",
    "swagger-ui-react": "^5.12.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "vaul": "^1.1.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@chromatic-com/storybook": "^4.1.1",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@faker-js/faker": "^10.0.0",
    "@mdx-js/mdx": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@mdx-js/rollup": "^3.1.1",
    "@mswjs/data": "^0.16.2",
    "@next/mdx": "^15.5.0",
    "@playwright/test": "^1.50.0",
    "@storybook/addon-a11y": "^9.1.3",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-vitest": "^9.1.3",
    "@storybook/jest": "^0.2.2",
    "@storybook/nextjs-vite": "^9.1.3",
    "@storybook/testing-library": "^0.2.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/swagger-ui-react": "^5.18.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.9.0",
    "eslint-config-next": "^15.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-mdx": "^2.3.4",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "mock-socket": "^9.3.1",
    "msw": "^2.10.5",
    "msw-storybook-addon": "^2.0.2",
    "playwright": "^1.55.0",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "remark-gfm": "^4.0.0",
    "storybook": "^9.1.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx,css,json}": [
      "prettier --write",
      "eslint --fix --max-warnings=0"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
